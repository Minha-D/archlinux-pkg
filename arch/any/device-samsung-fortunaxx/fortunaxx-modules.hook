# /etc/initcpio/hooks/fortunaxx-modules.hook
# These modules are requred to boot mainline on samsung fortunaxx devices to start display,charging,gpu..
run_hook() {
  modprobe panel-samsung-hx8389c-gh9607501a
  modprobe panel-samsung-s6d78a0-gh9607501a
  modprobe msm
  modprobe zinitix
  modprobe rt5033
  modprobe rt5033-charger
  # Add more modules as needed
}
